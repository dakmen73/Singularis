      {
        "name": "SVAS.Module",
        "type": "library",
        "targetFramework": "net10.0",
        "checkEolTargetFramework": false,
        "deterministic": false,
        "assemblyVersion": "1.0.*",
        "fileVersion": "1.0.0.0",
        "implicitUsings": true,
        "nugetPackages": [
          "DevExpress.ExpressApp",
          "DevExpress.ExpressApp.Chart",
          "DevExpress.ExpressApp.CloneObject",
          "DevExpress.ExpressApp.CodeAnalysis",
          "DevExpress.ExpressApp.ConditionalAppearance",
