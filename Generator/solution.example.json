        "name": "SVAS.E2E.Tests",
        "type": "test",
        "targetFramework": "net10.0-windows",
        "useWindowsForms": true,
        "isPackable": false,
        "checkEolTargetFramework": false,
        "implicitUsings": true,
        "dependencies": ["SVAS.Module"],
        "nugetPackages": [
          "coverlet.collector",
          "Microsoft.NET.Test.Sdk",
          "System.Net.Http",
          "System.Text.RegularExpressions",
          "xunit",
