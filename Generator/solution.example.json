            "namespace": "SVAS.WebApi"
          },
          {
            "path": "Startup.cs",
            "template": "custom",
            "namespace": "SVAS.WebApi"
          }
        ]
      },
      {
        "name": "SVAS.Win",
        "type": "win",
        "targetFramework": "net10.0-windows",
        "outputType": "WinExe",
        "useWindowsForms": true,
        "applicationIcon": "ExpressApp.ico",
        "checkEolTargetFramework": false,
        "deterministic": false,
        "assemblyVersion": "1.0.*",
        "fileVersion": "1.0.0.0",
        "implicitUsings": true,
        "dependencies": ["SVAS.Module"],
        "nugetPackages": [
          "DevExpress.ExpressApp",
          "DevExpress.ExpressApp.Chart.Win",
          "DevExpress.ExpressApp.CodeAnalysis",
          "DevExpress.ExpressApp.Dashboards.Win",
          "DevExpress.ExpressApp.FileAttachment.Win",
