          "DevExpress.Persistent.Base",
          "DevExpress.Persistent.BaseImpl.Xpo",
          "Azure.Identity",
          "Microsoft.Data.SqlClient",
          "System.IdentityModel.Tokens.Jwt"
        ],
        "embeddedResources": ["Model.DesignedDiffs.xafml"],
        "folders": ["BusinessObjects", "Controllers", "DatabaseUpdate", "Images"],
        "files": [
          {
            "path": "Module.cs",
            "template": "module-class",
            "namespace": "SVAS.Module"
          }
        ]
      },
      {
        "name": "SVAS.Blazor.Server",
        "type": "blazor",
        "targetFramework": "net10.0",
        "checkEolTargetFramework": false,
        "deterministic": false,
        "assemblyVersion": "1.0.*",
        "fileVersion": "1.0.0.0",
        "implicitUsings": true,
        "dependencies": ["SVAS.Module"],
        "nugetPackages": [
          "DevExpress.Drawing.Skia",
          "DevExpress.ExpressApp.Blazor",
          "DevExpress.ExpressApp.CodeAnalysis",
          "DevExpress.ExpressApp.Dashboards.Blazor",
          "DevExpress.ExpressApp.FileAttachment.Blazor",
          "DevExpress.ExpressApp.Office.Blazor",
          "DevExpress.ExpressApp.ReportsV2.Blazor",
          "DevExpress.ExpressApp.Security.Xpo",
          "DevExpress.ExpressApp.Validation.Blazor",
          "Microsoft.AspNetCore.Authentication.Negotiate"
        ],
        "contentFiles": [
          {
            "path": "Model.xafml",
            "copyToOutput": "Always"
          }
        ],
        "folders": ["Controllers", "Editors", "Images", "Pages", "Services"],
        "files": [
          {
            "path": "Program.cs",
            "template": "blazor-program",
            "namespace": "SVAS.Blazor.Server"
          },
          {
            "path": "BlazorApplication.cs",
            "template": "custom",
            "namespace": "SVAS.Blazor.Server"
          },
          {
            "path": "BlazorModule.cs",
            "template": "custom",
            "namespace": "SVAS.Blazor.Server"
          }
        ]
