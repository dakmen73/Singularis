          "DevExpress.ExpressApp.Win",
          "DevExpress.ExpressApp.Win.Design",
          "DevExpress.ExpressApp.Xpo",
          "DevExpress.Persistent.Base",
          "DevExpress.Persistent.BaseImpl.Xpo",
          "DevExpress.RichEdit.Export",
          "DevExpress.Win.Design"
        ],
        "usingAliases": [
          {
            "namespace": "DevExpress.ExpressApp.ListView",
            "alias": "ListView"
          }
        ],
        "contentFiles": [
          {
            "path": "Model.xafml",
            "copyToOutput": "Always"
          }
        ],
        "embeddedResources": [
          "Images/Logo.svg",
          "Images/ExpressAppLogo.png"
        ],
        "folders": ["Controllers", "Editors", "Images"],
        "files": [
          {
            "path": "Program.cs",
            "template": "win-program",
            "namespace": "SVAS.Win"
          },
          {
            "path": "WinApplication.cs",
            "template": "custom",
            "namespace": "SVAS.Win"
          },
          {
            "path": "WinModule.cs",
            "template": "custom",
            "namespace": "SVAS.Win"
          }
        ]
      },
